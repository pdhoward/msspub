{
  "name": "msspub",
  "version": "0.8.0",
  "description": "Kafka to Redis connector",
  "main": "server.js",
  "license": "MIT",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "cors": "^2.8.5",
    "dotenv": "^4.0.0",
    "express": "^4.16.4",
    "if-env": "^1.0.4",
    "ioredis": "^4.19.4",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "3.6.3",
    "node-fetch": "^2.6.0",
    "nodemon": "^2.0.7",
    "serve-favicon": "^2.5.0",
    "uuid": "^8.3.2",
    "valid-url": "^1.0.9",
    "ws": "^7.4.2"
  },
  "engines": {},
  "devDependencies": {
    "concurrently": "^6.2.0"
  }
}
